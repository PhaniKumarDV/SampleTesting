diff -urN linux-3.14-org/include/linux/if_vlan.h linux-3.14-mod/include/linux/if_vlan.h
--- linux-3.14-org/include/linux/if_vlan.h	2018-08-13 14:08:30.640588000 +0530
+++ linux-3.14-mod/include/linux/if_vlan.h	2018-09-19 12:46:31.905505703 +0530
@@ -24,6 +24,8 @@
 #define VLAN_ETH_HLEN	18		/* Total octets in header.	 */
 #define VLAN_ETH_ZLEN	64		/* Min. octets in frame sans FCS */
 
+#define VLAN_HLEN_PBB	6+6+4+2+4		/* The additional bytes required by PBB VLAN */
+
 /*
  * According to 802.3ac, the packet can be 4 bytes longer. --Klika Jan
  */
@@ -42,6 +44,23 @@
 
 /**
  *	struct vlan_ethhdr - vlan ethernet header (ethhdr + vlan_hdr)
+ *	@b_dest: Backbone destination ethernet address
+ *	@b_source: Backbone source ethernet address
+ *	@b_vlan_proto: ethernet protocol(0x88a8)
+ *	@b_vlan_TCI: priority and VLAN ID
+ *	@b_tpid: 0x88e7
+ *	@b_isid: Backbone VLAN Service ID
+ */
+struct vlan_ethhdr_pbb {
+	unsigned char	b_dest[ETH_ALEN];
+	unsigned char	b_source[ETH_ALEN];
+	__be16		b_vproto;
+	__be16		b_vid;
+	__be16		b_tpid;
+	__be32		b_isid;
+};
+/**
+ *	struct vlan_ethhdr - vlan ethernet header (ethhdr + vlan_hdr)
  *	@h_dest: destination ethernet address
  *	@h_source: source ethernet address
  *	@h_vlan_proto: ethernet protocol
@@ -281,7 +300,29 @@
 		return true;
 	return false;
 }
+/* vlan_insert_pbb_tag - VLAN PBB tag inserting
+ *@skb: skbuff to tag
+ */
 
+static inline struct sk_buff *vlan_insert_tag_pbb( struct sk_buff *skb )
+{
+	struct vlan_ethhdr_pbb *veth;
+
+	if (skb_cow_head(skb, VLAN_HLEN) < 0) {
+		kfree_skb(skb);
+		return NULL;
+	}
+	veth = (struct vlan_ethhdr_pbb *)skb_push(skb, VLAN_HLEN_PBB);
+
+	/* B-DA | B-SA | 0x88a8 | BVID | 0x88e7 | ISID */
+	memcpy(veth->b_dest, skb->pbb_da_mac, ETH_ALEN);
+	memcpy(veth->b_source, skb->pbb_sa_mac, ETH_ALEN);
+	veth->b_vproto = htons(ETH_P_8021AD);
+	veth->b_vid = htons(skb->pbb_bvid);
+	veth->b_tpid = htons(ETH_P_8021AH);
+	veth->b_isid = htons(skb->pbb_isid);
+	return skb;
+}
 /**
  * vlan_insert_tag - regular VLAN tag inserting
  * @skb: skbuff to tag
