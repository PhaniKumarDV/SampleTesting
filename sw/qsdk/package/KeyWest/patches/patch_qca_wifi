+            actionargs.action   = IEEE80211_ACTION_TX_PARAMS;
+            actionargs.arg1     = 0;
+            actionargs.arg2     = 0;
+            actionargs.arg3     = 0;
+            ieee80211_send_action( ni, &actionargs, NULL );
+        }
+    }
