+            eth_dev = dev_get_by_name(&init_net, "eth0");
+            if(eth_dev) {
+                *value = edma_get_rx_frame_overrun_errors(eth_dev);
+            }
+            dev_put(eth_dev);
+        }
+        break;
+    case IEEE80211_PARAM_KWN_ETH_TX_MULTICAST_PKT:
