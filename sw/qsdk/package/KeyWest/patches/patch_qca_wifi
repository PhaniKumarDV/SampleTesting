+
     vap->offchan_requestor = 0;
     vap->iv_csmode = IEEE80211_CSA_MODE_AUTO; /* csmode will be calculated dynamically */
     vap->iv_enable_ecsaie = true; /* Extended channel switch ie is enabled by default */
@@ -552,7 +1023,18 @@
