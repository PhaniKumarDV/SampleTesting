+                        memset(&wreq, 0, sizeof(wreq));
+                        wreq.addr.sa_family = ARPHRD_ETHER;
+                        memcpy(wreq.addr.sa_data, ni->ni_macaddr, 6);
+                        wreq.addr.sa_data[6] = IEEE80211_EV_DYING_GASP;
+                        wireless_send_event(dev, IWEVEXPIRED, &wreq, NULL);
+                    }
