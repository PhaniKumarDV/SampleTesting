config vlan 'vlan'
    option status '1'
    option mode '0'
    option mgmtvlan '1'
    option accessvlan '10'
    option trunkoption '2'
    option svlan '100'
    option svlanethertype '0x0081'
    option bda '00:00:00:00:00:00'
    option bsa '00:00:00:00:00:00'
    option bvid '-1'
    option bisid '-1'

