config tftp 'tftp'
    option filename 'config.txt'
    option filetype '1'
    option serverip '192.168.1.100'
    option opstatus '1'
    option optype   '0'

config http 'http'
    option filetype 1
    option optype 0

config retip 'retip'
    option retainip 0

config firmware 'firmware'
    option security 0
