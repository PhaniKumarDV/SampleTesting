config system
	option hostname	Sify
	option timezone	IST-5:30
	option log_size	64
    option templogstatus 1
    option temploginterval 30
    option temperature 0
    option log_ip '192.168.1.100'
    option log_port '514'
    option location 'location'
    option contact '1234567890'
    option email 'example@mail.com'
    option bstid 'mh-mum-air-T1-st'

config timeserver ntp
	#list server	0.openwrt.pool.ntp.org
	#list server	1.openwrt.pool.ntp.org
	#list server	2.openwrt.pool.ntp.org
	#list server	3.openwrt.pool.ntp.org
	option enabled 1
	option enable_server 0

config gps 'gps'
        option status 1
        option interval 60
        option latitude 0
        option longitude 0
        option altitude 0
        option time "No Value"
        option satelites 0

config led 'led'
        option status 1

config customer 'customer'
        option name "Customer Name"
        option linkid 0
